        </script><script type="text/javascript">'use strict';

function isIE() {
  return (window.navigator.userAgent.match(/MSIE|Trident/) !== null);
}

// forEach - https://developer.mozilla.org/en-US/docs/Web/JavaScript/Reference/Global_Objects/Array/forEach#polyfil
Array.prototype.forEach || (Array.prototype.forEach = function (r, o) { if (null == this) throw new TypeError("Array.prototype.forEach called on null or undefined"); var t, n, e = Object(this), a = e.length >>> 0; if ("function" != typeof r) throw new TypeError(r + " is not a function"); for (arguments.length > 1 && (t = o), n = 0; n < a;) { var f; n in e && (f = e[n], r.call(t, f, n, e)), n++ } });

// missing forEach on NodeList for IE11
if (window.NodeList && !NodeList.prototype.forEach) {
  NodeList.prototype.forEach = Array.prototype.forEach;
}

if (isIE()) {
  console.log('Is Internet Explorer');
}

if (typeof window.IntersectionObserver === 'undefined') {
  console.log('no IntersectionObserver');
  var script = document.createElement('script');
  script.type = 'text/javascript';
  script.src = 'https://polyfill.io/v3/polyfill.min.js?features=IntersectionObserver';
  document.getElementsByTagName('head')[0].appendChild(script);
}
</script><script type="text/javascript">function OptanonWrapper() {
  handleScroll();
  notifyConsentChange();
  window.Optanon.OnConsentChanged(() => {
    notifyConsentChange();
    document.body.style.overflowY = 'auto';
  });
}
// https://community.cookiepro.com/s/article/UUID-534defd9-7ec8-fc0e-78bf-8dce8c6c5593
// WARNING: The documentation is wrong! "pcSaveBtn" doesn't have the class "button-theme"
// WARNING: The documentation is wrong! The event on "pcSaveBtn" is never called :(
function addBannerStatusEventListener(callback) {
  function getCookie(name) {
    const value = `; ${document.cookie}`;
    const parts = value.split(`; ${name}=`);
    if (parts.length === 2) return parts.pop().split(";").shift();
    return null;
  }

  const bannerAcceptBtn = document.getElementById("onetrust-accept-btn-handler");
  const bannerCloseBtn = document.getElementsByClassName("onetrust-close-btn-handler banner-close-button")[0];
  const pcAllowAllBtn = document.getElementById("accept-recommended-btn-handler");
  const pcSaveBtn = document.getElementsByClassName("save-preference-btn-handler onetrust-close-btn-handler")[0];

  // const OABCcookieName = "OptanonAlertBoxClosed";
  // const OABCcookie = getCookie(OABCcookieName);

  if (bannerAcceptBtn) {
    bannerAcceptBtn.addEventListener('click', function () {
      console.log("Cookie Banner - Allowed all via Banner");
      callback();
    });
  } else {
    console.error('Cookie Banner - Click listener not set on "bannerAcceptBtn"');
  }

  if (bannerCloseBtn) {
    bannerCloseBtn.addEventListener('click', function () {
      console.log("Cookie Banner - Banner closed");
      callback();
    });
  } else {
    console.error('Cookie Banner - Click listener not set on "bannerCloseBtn"');
  }

  if (pcAllowAllBtn) {
    pcAllowAllBtn.addEventListener('click', function () {
      console.log("Cookie Banner - Allowed all via Preference Center");
      callback();
    });
  } else {
    console.error('Cookie Banner - Click listener not set on "pcAllowAllBtn"');
  }

  if (pcSaveBtn) {
    pcSaveBtn.addEventListener('click', function () {
      console.log("Cookie Banner - Set custom settings via Preference Center");
      callback();
    });
  } else {
    console.error('Cookie Banner - Click listener not set on "pcSaveBtn"');
  }
}

function handleScroll() {
  // Do nothing if the banner is already closed
  if (window.Optanon.IsAlertBoxClosed()) return;

  // Disable scroll
  document.body.style.overflowY = 'hidden';
  // document.body.style.position = 'fixed'; // fix iOS

  window.dispatchEvent(new Event('resize'));

  addBannerStatusEventListener(() => {
    // Always enable scroll on click
    // const closed = Optanon.IsAlertBoxClosed();
    // if (!closed) {
    //   console.warn('Cookie Banner - not closed')
    //   return;
    // }

    // Enable scroll
    document.body.style.overflowY = 'auto';
    // document.body.style.position = ''; // fix iOS
  });
}

function notifyConsentChange() {
  let groupIds = [];
  if (window.OnetrustActiveGroups) {
    groupIds = window.OnetrustActiveGroups.split(',').filter(Boolean);
  }
  window.dispatchEvent(new CustomEvent('onetrust-notify-consent-change', {
    detail: {
      groupIds
    }
  }))
}

function openOneTrustSettings(ev) {
  const oneTrustDarkback = document.querySelector('#onetrust-consent-sdk>.onetrust-pc-dark-filter');
  const onetrustPcSdk = document.querySelector('#onetrust-pc-sdk');
  const bodyTag = document.querySelector('body');
  if (!(oneTrustDarkback && onetrustPcSdk)) return false;
  ev?.preventDefault();
  oneTrustDarkback.style.opacity = 1;
  oneTrustDarkback.style.visibility = 'visible';
  oneTrustDarkback.style.display = 'block';
  onetrustPcSdk.style.opacity = 1;
  onetrustPcSdk.style.visibility = 'visible';
  onetrustPcSdk.style.height = '80vh'; // TODO: ?? on mobile is wrong 80vh
  onetrustPcSdk.style.display = 'block';
  bodyTag.style.overflow = 'hidden';
  oneTrustDarkback.classList.remove('ot-hide');
  onetrustPcSdk.classList.remove('ot-hide');
  return false;
}
